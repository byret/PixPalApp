<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SavePaletteController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="220.0" prefWidth="210.0" styleClass="pane">
         <children>
            <Label layoutX="30.0" layoutY="15.0" prefHeight="21.0" prefWidth="150.0" style="-fx-font-family: 'rainyhearts';" text="Palette name:">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <TextField fx:id="name" layoutX="30.0" layoutY="40.0" prefHeight="25.0" prefWidth="150.0" />
            <CheckBox fx:id="checkBox" layoutX="30.0" layoutY="135.0" mnemonicParsing="false" onAction="#checkBoxPressed" style="-fx-font-family: 'rainyhearts';" text="Save to cloud">
               <font>
                  <Font size="15.0" />
               </font></CheckBox>
            <Button layoutX="50.0" layoutY="175.0" mnemonicParsing="false" onAction="#save" prefHeight="25.0" prefWidth="111.0" style="-fx-font-family: 'rainyhearts';" text="Save">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Label fx:id="categoryLabel" disable="true" layoutX="30.0" layoutY="75.0" prefHeight="21.0" prefWidth="150.0" style="-fx-font-family: 'rainyhearts';" text="Category:">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <TextField fx:id="category" disable="true" layoutX="30.0" layoutY="100.0" prefHeight="25.0" prefWidth="150.0" />
         </children>
      </Pane>
   </children>
</GridPane>
